        "x = frequence['df_frequence']\n",
        "y = frequence['age_client']\n",
        "slope, intercept, r, p, se = stats.linregress(x, y)\n",
        "\n",
        "print(f\"R-squared: {r**2:.6f}\")\n",
        "\n",
        "def predict(x):\n",
        "  return slope * x + intercept"
