        "# On calcul khi2 = st_chi2\n",
        "# V√©rifier si les valeurs sont corr√©l√©es, nous devons passer une √©tape supplementaire appel√©e \"seuil de significativit√©\"\n",
        "# La Pvalue : l'unique valeur n√©cessaire pour prendre notre d√©cision = st_p\n",
        "# Nombre de degr√©s de libert√© = st_dof\n",
        "# Le tableau des fr√©quences attendues = st_exp\n",
        "\n",
        "from scipy import stats\n",
        "\n",
        "st_chi2, st_p, st_dof, st_exp = stats.chi2_contingency(tab_contingence)\n",
        "\n",
