        "id": "hGyR5lE8hnMu"
      },
      "outputs": [],
      "source": [
        "# Correlation entre l’age et la frequence d'achat des clients\n",
        "frequence_achat = librairie.groupby('age_client').count().reset_index()\n",
        "\n",
        "frequence = frequence_achat[['age_client', 'price']]\n",
        "\n",
        "frequence['df_frequence'] = frequence['price']/sum(frequence['price'])\n",
        "frequence.sort_values(by='age_client', ascending=False)"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": null,
      "metadata": {
        "id": "D0uyeGXmxpkx"
      },
      "outputs": [],
      "source": [
        "# Graphique représentant la frequence d'achat\n",
        "plt.title('Frequence d achats selon l age du client')\n",
        "sns.lineplot(x=frequence.age_client, y=frequence.price)"
      ]
    },
    {
      "cell_type": "code",
